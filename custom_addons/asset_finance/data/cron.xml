<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_asset_finance_late_interest" model="ir.cron">
            <field name="name">Calculate Late Interest</field>
            <field name="model_id" ref="model_finance_contract"/>
            <field name="state">code</field>
            <field name="code">model._cron_calculate_late_interest()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="ir_cron_asset_finance_apply_penalties" model="ir.cron">
            <field name="name">Finance: Apply Late Payment Penalties</field>
            <field name="model_id" ref="model_finance_contract"/>
            <field name="state">code</field>
            <field name="code">model._cron_apply_late_penalties()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>